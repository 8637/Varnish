
test "Test backslash escapes in VCL"

server s1 {
	rxreq 
	txresp
} -start

varnish v1 -vcl+backend { 
	sub vcl_deliver {
		set resp.http.foo1 = "abcd\\abcd";
		set resp.http.foo2 = "abcd\"abcd";
	}
} -start 

client c1 {
	txreq
	rxresp
	expect resp.http.foo1 == "abcd\\abcd"
	expect resp.http.foo2 == "abcd\"abcd"
} -run
